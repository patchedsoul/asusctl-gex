<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node name="/org/rogcore/Daemon">
  <interface name="org.freedesktop.DBus.Introspectable">
    <method name="Introspect">
      <arg name="xml_data" type="s" direction="out"/>
    </method>
  </interface>
  <interface name="org.rogcore.Daemon">
    <method name="AnimatrixWrite">
      <arg name="bytearray1" type="ay" direction="in"/>
      <arg name="bytearray2" type="ay" direction="in"/>
      <annotation name="org.freedesktop.DBus.Method.NoReply" value="true"/>
    </method>
    <method name="GetChargeLimit">
      <arg name="value" type="y" direction="out"/>
    </method>
    <method name="GetFanMode">
      <arg name="value" type="y" direction="out"/>
    </method>
    <method name="GetKeyBacklight">
      <arg name="json" type="s" direction="out"/>
    </method>
    <method name="GetKeyBacklightModes">
      <arg name="json" type="s" direction="out"/>
    </method>
    <method name="SetChargeLimit">
      <arg name="value" type="y" direction="in"/>
      <annotation name="org.freedesktop.DBus.Method.NoReply" value="true"/>
    </method>
    <method name="SetFanMode">
      <arg name="value" type="y" direction="in"/>
      <annotation name="org.freedesktop.DBus.Method.NoReply" value="true"/>
    </method>
    <method name="SetKeyBacklight">
      <arg name="json" type="s" direction="in"/>
      <annotation name="org.freedesktop.DBus.Method.NoReply" value="true"/>
    </method>
    <signal name="ChargeLimitChanged">
      <arg name="limit" type="y"/>
    </signal>
    <signal name="FanModeChanged">
      <arg name="mode" type="y"/>
    </signal>
    <signal name="KeyBacklightChanged">
      <arg name="json" type="s"/>
    </signal>
  </interface>
</node>
